      <Button fx:id="btn_bitir" layoutX="140.0" layoutY="256.0" mnemonicParsing="false" onMouseClicked="#btn_bitir_Clicked" prefHeight="26.0" prefWidth="150.0" text="Ödeme Bitir" />
